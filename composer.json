{
	"name" : "wabel/zoho-crm-copy-db",
	"description" : "This package contains code to help you copy ZohoCRM records directly into your database.",
	"type" : "library",
	"license" : "MIT",
	"keywords" : [
	  	"Zoho CRM",
		"ZohoCRM",
		"Zoho",
		"ORM",
		"Copy"
	],
	"require" : {
		"php" : ">=5.6",
		"wabel/zoho-crm-orm": "~1.1",
		"doctrine/dbal": "~2.5",
		"symfony/console": "~2.0 | ~3.0",
		"mouf/utils.common.lock": "~1.0",
		"danielstjules/stringy": "~2.2"
	},
    "require-dev" : {
	  "phpunit/phpunit": "~4.0",
	  "satooshi/php-coveralls": "~1.0"
	},
	"autoload" : {
		"psr-4" : {
			"Wabel\\Zoho\\CRM\\Copy\\" : "src"
		}
	},
    "autoload-dev" : {
  	    "psr-4" : {
		    "Wabel\\Zoho\\CRM\\Copy\\" : "tests"
	    }
    },
  	"minimum-stability": "dev",
    "prefer-stable": true
}
